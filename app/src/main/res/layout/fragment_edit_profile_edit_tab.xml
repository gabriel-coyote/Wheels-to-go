<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_backround">

    <View
        android:id="@+id/view"
        android:layout_width="1700dp"
        android:layout_height="2dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.107" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="308dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/roboto"
        android:text="My Profile"
        android:textColor="#FFFFFF"
        android:textSize="60sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="260dp"
        android:layout_marginEnd="836dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_profile" />

    <EditText
        android:id="@+id/fist_name"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="504dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="First"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/sex"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="752dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="Sex"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="732dp"
        android:layout_marginTop="144dp"
        android:text="Edit Profile"
        android:textColor="#FFFFFF"
        android:textSize="45sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/address"
        android:layout_width="500dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/ic_rectangle_large"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPostalAddress"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location" />

    <TextView
        android:id="@+id/button_save"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="916dp"
        android:background="@drawable/ic_rectangle_save"
        android:ems="10"
        android:gravity="center"
        android:text="Save"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/birth_date"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="752dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="Date Of Birth"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/sex"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/location"
        android:layout_width="500dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="592dp"
        android:background="@drawable/ic_rectangle_large"
        android:ems="10"
        android:hint="Dealer Location"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/last_name"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="504dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="Last"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/fist_name"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/eployee_ID"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="732dp"
        android:layout_marginTop="836dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="Employee ID"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/SSN"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="836dp"
        android:background="@drawable/ic_rectangle_small"
        android:ems="10"
        android:hint="SSN"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/eployee_ID"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>